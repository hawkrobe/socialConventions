@article{VonFintel08_UniversalsSemantics,
	Abstract = {This article surveys the state of the art in the field of semantic universals. We examine potential semantic universals in three areas: (i) the lexicon, (ii) se- mantic ``glue'' (functional morphemes and composition principles), and (iii) pragmatics. At the level of the lexicon, we find remarkably few convincing se- mantic universals. At the level of functional morphemes and composition prin- ciples, we discuss a number of promising constraints, most of which require further empirical testing and/or refinement. In the realm of pragmatics, we predict that Gricean mechanisms are universal, but suggest that the precise nature of presuppositions may be subject to cross-linguistic variation. Finally, we follow E.L. Keenan in concluding that the overarching universal of effabil- ity or translatability between languages cannot be upheld in its strongest form. A recurring theme throughout this survey is how much work still remains to be done in the relatively young field of cross-linguistic formal semantics.},
	Annote = {optional reading for ling230a

},
	Author = {Von Fintel, Kai and Matthewson, Lisa},
	Date-Added = {2015-02-06 00:38:03 +0000},
	Date-Modified = {2015-02-06 00:39:48 +0000},
	Journal = {The Linguistic Review},
	Number = {1-2},
	Pages = {139--201},
	Title = {Universals in semantics},
	Volume = {25},
	Year = {2008},
	Bdsk-File-1 = {}}

